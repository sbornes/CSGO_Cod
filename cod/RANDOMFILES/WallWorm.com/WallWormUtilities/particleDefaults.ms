	rollout wwmtParticlesDefaults "WWMT Default Particles" width:256 height:428
	(
		ComboBox cbx_particleList "Particle Effects" pos:[13, 70] width:224 height:22
		Button btn_use "Use Effect" pos:[13, 387] width:223 height:31
		DropDownList ddl1 "Mod Particle Repository" pos:[12, 14] width:222 height:40 selection:2
		on btn_use pressed do
		(
			if cbx_particleList.selected != undefined AND cbx_particleList.selected != "" then (
				--try (
				global wwToolsFloater
				wwToolsFloater.rollouts[6].setParticleNames cbx_particleList.selected
				--) catch (
				--messagebox "There was a problem"
				--)
			)

		)
		on ddl1 selected sel do
		(
			cbx_particleList.items = wwmt_particles_default_names[sel]

		)
	)

	--global wwmtParticlesDefaults

	createDialog wwmtParticlesDefaults

	wwmtParticlesDefaults.ddl1.items = wwmt_particles_mod_map

	wwmtParticlesDefaults.cbx_particleList.items = wwmt_particles_default_names[wwmtParticlesDefaults.ddl1.selection]

)